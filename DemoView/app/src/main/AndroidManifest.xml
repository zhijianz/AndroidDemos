<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zzj.demoview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MoveViewActivity" />
        <activity android:name=".CanvasDrawActivity" />
        <activity android:name=".ScrollActivity" />
        <activity android:name=".LevelActivity" />
        <activity android:name=".TypefaceActivity" />
        <activity android:name=".AutoFitActivity"></activity>
    </application>

</manifest>